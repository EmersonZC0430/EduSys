AA;AACA,KApBA;AAqBA,IAAA,MArBA,oBAqBA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,UAAA,GAAA;AACA,QAAA,EAAA,EAAA,CAAA,CADA;AAEA,QAAA,QAAA,EAAA,EAFA;AAGA,QAAA,SAAA,EAAA,EAHA;AAIA,QAAA,QAAA,EAAA;AAJA,OAAA;AAOA,WAAA,OAAA,GAAA,IAAA;AACA,KA/BA;AAgCA,IAAA,IAhCA,gBAgCA,EAhCA,EAgCA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,GAAA;;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAJA;AAKA,KAtCA;AAuCA,IAAA,WAvCA,uBAuCA,IAvCA,EAuCA;AACA,UAAA,KAAA,GAAA,EAAA;;AACA,WAAA,IAAA,GAAA,GAAA,CAAA,EAAA,GAAA,GAAA,UAAA,CAAA,eAAA,EAAA,GAAA,EAAA,EAAA;AACA,YAAA,CAAA,KAAA,GAAA,GAAA,IAAA,CAAA,SAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,CAAA,KAAA,GAAA,EAAA,QAAA,EAAA;AACA;AACA;;AAEA,WAAA,QAAA,GAAA,KAAA;AACA,KAhDA;AAiDA,IAAA,sBAjDA,oCAiDA;AACA,UAAA,SAAA,GAAA,CAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,QAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,SAAA,IAAA,QAAA,CAAA,KAAA,QAAA,CAAA,CAAA,CAAA,CAAA;AACA;;AACA,aAAA,SAAA;AACA,KAvDA;AAwDA,IAAA,IAxDA,kBAwDA;AAAA;;AACA,WAAA,UAAA,CAAA,SAAA,GAAA,KAAA,sBAAA,EAAA;;AACA,UAAA,KAAA,UAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA;AACA,KAnEA;AAoEA,IAAA,UApEA,wBAoEA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,IAAA;AACA,WAAA,OAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KAxEA;AAyEA,IAAA,UAzEA,sBAyEA,EAzEA,EAyEA;AAAA;;AACA,MAAA,GAAA,CAAA,UAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OAHA;AAIA;AA9EA,GAVA;AA0FA,EAAA,OA1FA,qBA0FA;AACA,SAAA,OAAA;AACA;AA5FA,CAAA","sourcesContent":["<template>\n  <div class=\"admin-wrap\">\n    <div class=\"crumbs\">\n      <el-breadcrumb separator=\"/\">\n        <el-breadcrumb-item>\n          <i class=\"el-icon-fa fa-id-badge\"></i> 管理员管理\n        </el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n\n    <div class=\"container\">\n      <div class=\"query-form\">\n        <el-row :gutter=\"20\">\n          <el-col :span=\"2\">\n            <el-button @click=\"create\" icon=\"el-icon-plus\">创建</el-button>\n          </el-col>\n        </el-row>\n      </div>\n\n      <div class=\"table\">\n        <el-table :data=\"tableData\" stripe max-height=\"500\">\n          <el-table-column label=\"管理员Id\" prop=\"id\" />\n          <el-table-column label=\"管理员用户名\" prop=\"username\" />\n          <el-table-column label=\"权限个数\" prop=\"permission\" />\n          <el-table-column align=\"center\" label=\"操作\" width=\"200px\">\n            <template slot-scope=\"scope\">\n              <el-button @click=\"edit(scope.row.id)\" size=\"mini\" type=\"success\"\n                >编辑\n              </el-button>\n              <el-button\n                @click=\"deleteItem(scope.row.id)\"\n                size=\"mini\"\n                type=\"danger\"\n                >删除\n              </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n\n      <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n        <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"entityForm.username\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"权限\">\n            <el-checkbox-group v-model=\"priGroup\">\n              <el-checkbox label=\"1\" name=\"priGroup\">系部管理</el-checkbox>\n              <el-checkbox label=\"2\" name=\"priGroup\">专业管理</el-checkbox>\n              <el-checkbox label=\"4\" name=\"priGroup\">班级管理</el-checkbox>\n              <el-checkbox label=\"8\" name=\"priGroup\">学生管理</el-checkbox>\n              <el-checkbox label=\"16\" name=\"priGroup\">教师管理</el-checkbox>\n              <el-checkbox label=\"32\" name=\"priGroup\">课程管理</el-checkbox>\n              <el-checkbox label=\"64\" name=\"priGroup\">选课修改</el-checkbox>\n              <el-checkbox label=\"128\" name=\"priGroup\">管理员管理</el-checkbox>\n            </el-checkbox-group>\n          </el-form-item>\n          <el-form-item label=\"密码\">\n            <el-input type=\"password\" v-model=\"entityForm.password\"></el-input>\n          </el-form-item>\n        </el-form>\n        <span class=\"dialog-footer\" slot=\"footer\">\n          <el-button @click=\"save\" type=\"primary\">确 定</el-button>\n          <el-button @click=\"editing = false\">取 消</el-button>\n        </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/admin\";\nimport Permission from \"../../common/permission\";\n\nexport default {\n  name: \"AdminAdmin\",\n  data() {\n    return {\n      entityForm: {},\n      priGroup: [],\n      tableData: [],\n      editing: false,\n    };\n  },\n  methods: {\n    getList() {\n      api.list().then((res) => {\n        res = this.addPermissionProp(res);\n        this.tableData = res;\n      });\n    },\n\n    addPermissionProp(data) {\n      for (let i = 0; i < data.length; i++) {\n        let count = 0;\n        for (let bit = 0; bit < Permission.permissionCount; bit++) {\n          if (((1 << bit) & data[i].privilege) === 1 << bit) {\n            count++;\n          }\n        }\n        data[i].permission = count;\n      }\n\n      return data;\n    },\n    create() {\n      this.priGroup = [];\n      this.entityForm = {\n        id: -1,\n        username: \"\",\n        privilege: \"\",\n        password: \"\",\n      };\n\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then((res) => {\n        this.setPriGroup(res);\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    setPriGroup(data) {\n      let group = [];\n      for (let bit = 0; bit < Permission.permissionCount; bit++) {\n        if (((1 << bit) & data.privilege) === 1 << bit) {\n          group.push((1 << bit).toString());\n        }\n      }\n\n      this.priGroup = group;\n    },\n    readPrivilegeFromGroup() {\n      let privilege = 0;\n      for (let i = 0; i < this.priGroup.length; i++) {\n        privilege += parseInt(this.priGroup[i]);\n      }\n      return privilege;\n    },\n    save() {\n      this.entityForm.privilege = this.readPrivilegeFromGroup();\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getList();\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getList();\n      });\n    },\n  },\n  created() {\n    this.getList();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/admin"}]}