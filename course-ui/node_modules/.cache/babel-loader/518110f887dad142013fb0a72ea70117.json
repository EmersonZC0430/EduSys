qBA,EArBA,EAqBA;AAAA;;AACA,MAAA,GAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,SAAA;AACA,OAHA;AAIA;AA1BA,GAdA;AA0CA,EAAA,OA1CA,qBA0CA;AACA,SAAA,KAAA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n  <div class=\"course-select-wrap\">\n    <div class=\"crumbs\">\n      <el-breadcrumb separator=\"/\">\n        <el-breadcrumb-item>\n          <i class=\"el-icon-fa fa-book\"></i> 选修课程\n        </el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n\n    <div class=\"container\">\n      <div class=\"query-form\">\n        <el-row :gutter=\"20\">\n          <el-col :offset=\"15\" :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"课程名\"\n              v-model=\"queryForm.courseName\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"教师名\"\n              v-model=\"queryForm.teacherName\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-button @click=\"query\" icon=\"el-icon-search\" type=\"primary\"\n              >搜索\n            </el-button>\n          </el-col>\n        </el-row>\n      </div>\n\n      <div class=\"table\">\n        <el-table :data=\"tableData\" stripe max-height=\"500\">\n          <el-table-column label=\"课程Id\" prop=\"courseId\" />\n          <el-table-column label=\"课程名\" prop=\"courseName\" width=\"180px\" />\n          <el-table-column label=\"教师\" prop=\"teacherName\" />\n          <el-table-column label=\"学分\" prop=\"credit\" />\n          <el-table-column\n            align=\"center\"\n            label=\"上课时间\"\n            prop=\"time\"\n            width=\"130px\"\n          />\n          <el-table-column label=\"已选人数\" prop=\"selectedCount\" />\n          <el-table-column label=\"课程容量\" prop=\"maxSize\" />\n          <el-table-column align=\"center\" label=\"操作\" width=\"200px\">\n            <template slot-scope=\"scope\">\n              <el-button\n                @click=\"select(scope.row.courseId)\"\n                size=\"mini\"\n                type=\"success\"\n                >选修\n              </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <br />\n      <el-row justify=\"center\" type=\"flex\">\n        <el-pagination\n          :current-page.sync=\"pageIndex\"\n          :page-size=\"pageSize\"\n          :total=\"pageSize * pageCount\"\n          @current-change=\"getPage\"\n          background\n          layout=\"prev, pager, next\"\n        >\n        </el-pagination>\n      </el-row>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"../../api/student/courseSelect\";\n\nexport default {\n  name: \"StudentCourseSelect\",\n  data() {\n    return {\n      queryForm: {\n        courseName: \"\",\n        teacherName: \"\",\n      },\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n    };\n  },\n  methods: {\n    query() {\n      api\n        .getPageCount(this.queryForm.courseName, this.queryForm.teacherName)\n        .then((res) => {\n          this.pageCount = res;\n          this.pageIndex = 1;\n          this.getPage(1);\n        });\n    },\n    getPage(pageIndex) {\n      api\n        .getPage(\n          pageIndex,\n          this.queryForm.courseName,\n          this.queryForm.teacherName\n        )\n        .then((res) => {\n          this.tableData = res;\n        });\n    },\n    select(id) {\n      api.select(id).then(() => {\n        this.$message.success(\"选修成功!\");\n        this.getPage(this.pageIndex);\n      });\n    },\n  },\n  created() {\n    this.query();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/student"}]}