m>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"../../api/student/info\";\n\nexport default {\n  name: \"StudentInfo\",\n  data() {\n    return {\n      entityForm: {\n        number: \"\",\n        name: \"\",\n        departmentName: \"\",\n        majorName: \"\",\n        className: \"\",\n        email: \"\",\n        birthday: \"\",\n        sex: \"\",\n        password: null\n      }\n    };\n  },\n  methods: {\n    get() {\n      api.get().then(res => {\n        this.entityForm = res;\n      });\n    },\n    update() {\n      api.update(this.entityForm).then(() => {\n        this.$message.success(\"更新信息成功!\");\n      });\n    }\n  },\n  created() {\n    this.get();\n  }\n};\n</script>\n\n<style scoped>\n.info-form {\n  min-width: 400px;\n  width: 35%;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/views/student"}]}