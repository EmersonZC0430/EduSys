A,KAAA,CAAA,OAAA,CAAA,CAAA;AACA,OAVA;AAWA,KAbA;AAcA,IAAA,OAdA,mBAcA,SAdA,EAcA;AAAA;;AACA,MAAA,GAAA,CACA,OADA,CAEA,SAFA,EAGA,KAAA,SAAA,CAAA,cAHA,EAIA,KAAA,SAAA,CAAA,SAJA,EAKA,KAAA,SAAA,CAAA,IALA,EAOA,IAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA;AACA,OATA;AAUA,KAzBA;AA0BA,IAAA,MA1BA,oBA0BA;AACA,WAAA,UAAA,GAAA;AACA,QAAA,EAAA,EAAA,CAAA,CADA;AAEA,QAAA,IAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,IAHA;AAIA,QAAA,OAAA,EAAA;AAJA,OAAA;AAMA,WAAA,OAAA,GAAA,IAAA;AACA,KAlCA;AAmCA,IAAA,IAnCA,gBAmCA,EAnCA,EAmCA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAHA;AAIA,KAxCA;AAyCA,IAAA,IAzCA,kBAyCA;AAAA;;AACA,UAAA,KAAA,UAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA;AACA,KAnDA;AAoDA,IAAA,UApDA,wBAoDA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,IAAA;AACA,WAAA,OAAA,CAAA,KAAA,SAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KAxDA;AAyDA,IAAA,UAzDA,sBAyDA,EAzDA,EAyDA;AAAA;;AACA,MAAA,GAAA,CAAA,UAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,SAAA;AACA,OAHA;AAIA,KA9DA;AA+DA,IAAA,SA/DA,uBA+DA;AAAA;;AACA,MAAA,QAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,GAAA;AACA,OAFA;AAGA;AAnEA,GAlBA;AAuFA,EAAA,OAvFA,qBAuFA;AACA,SAAA,KAAA;AACA,SAAA,SAAA;AACA;AA1FA,CAAA","sourcesContent":["<template>\n  <div class=\"class-wrap\">\n    <div class=\"crumbs\">\n      <el-breadcrumb separator=\"/\">\n        <el-breadcrumb-item>\n          <i class=\"el-icon-fa el-icon-fa fa-group\"></i> 班级管理\n        </el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n\n    <div class=\"container\">\n      <div class=\"query-form\">\n        <el-row :gutter=\"20\">\n          <el-col :span=\"2\">\n            <el-button @click=\"create\" icon=\"el-icon-plus\">创建</el-button>\n          </el-col>\n          <el-col :offset=\"10\" :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"班级名\"\n              v-model=\"queryForm.name\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"系名\"\n              v-model=\"queryForm.departmentName\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"专业名\"\n              v-model=\"queryForm.majorName\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-button @click=\"query\" icon=\"el-icon-search\" type=\"primary\"\n              >搜索\n            </el-button>\n          </el-col>\n        </el-row>\n      </div>\n\n      <div class=\"table\">\n        <el-table :data=\"tableData\" stripe max-height=\"500\">\n          <el-table-column label=\"班级Id\" prop=\"id\" />\n          <el-table-column label=\"班级名\" prop=\"name\" />\n          <el-table-column label=\"年级\" prop=\"grade\" />\n          <el-table-column label=\"所属系\" prop=\"departmentName\" />\n          <el-table-column label=\"所属专业\" prop=\"majorName\" />\n          <el-table-column align=\"center\" label=\"操作\" width=\"200px\">\n            <template slot-scope=\"scope\">\n              <el-button @click=\"edit(scope.row.id)\" size=\"mini\" type=\"success\"\n                >编辑\n              </el-button>\n              <el-button\n                @click=\"deleteItem(scope.row.id)\"\n                size=\"mini\"\n                type=\"danger\"\n                >删除\n              </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <br />\n      <el-row justify=\"center\" type=\"flex\">\n        <el-pagination\n          :current-page.sync=\"pageIndex\"\n          :page-size=\"pageSize\"\n          :total=\"pageSize * pageCount\"\n          @current-change=\"getPage\"\n          background\n          layout=\"prev, pager, next\"\n        >\n        </el-pagination>\n      </el-row>\n\n      <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n        <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n          <el-form-item label=\"班级名\">\n            <el-input v-model=\"entityForm.name\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"年级\">\n            <el-input type=\"number\" v-model=\"entityForm.grade\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"所属专业\">\n            <el-select placeholder=\"请选择专业\" v-model=\"entityForm.majorId\">\n              <el-option\n                :key=\"index\"\n                :label=\"item.name\"\n                :value=\"item.id\"\n                v-for=\"(item, index) in majors\"\n              />\n            </el-select>\n          </el-form-item>\n        </el-form>\n        <span class=\"dialog-footer\" slot=\"footer\">\n          <el-button @click=\"save\" type=\"primary\">确 定</el-button>\n          <el-button @click=\"editing = false\">取 消</el-button>\n        </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/class\";\nimport * as majorApi from \"../../api/admin/major\";\n\nexport default {\n  name: \"AdminClass\",\n  data() {\n    return {\n      queryForm: {\n        departmentName: \"\",\n        majorName: \"\",\n        name: \"\",\n      },\n      entityForm: {},\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n      editing: false,\n      majors: [],\n    };\n  },\n  methods: {\n    query() {\n      api\n        .getPageCount(\n          this.queryForm.departmentName,\n          this.queryForm.majorName,\n          this.queryForm.name\n        )\n        .then((res) => {\n          this.pageCount = res;\n          this.pageIndex = 1;\n          this.getPage(1);\n        });\n    },\n    getPage(pageIndex) {\n      api\n        .getPage(\n          pageIndex,\n          this.queryForm.departmentName,\n          this.queryForm.majorName,\n          this.queryForm.name\n        )\n        .then((res) => {\n          this.tableData = res;\n        });\n    },\n    create() {\n      this.entityForm = {\n        id: -1,\n        name: \"\",\n        grade: 2019,\n        majorId: \"\",\n      };\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then((res) => {\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    },\n    getMajors() {\n      majorApi.listName().then((res) => {\n        this.majors = res;\n      });\n    },\n  },\n  created() {\n    this.query();\n    this.getMajors();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/admin"}]}