A,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA;AACA,OAJA;AAKA,KAhBA;AAiBA,IAAA,MAjBA,oBAiBA;AACA,WAAA,UAAA,GAAA;AACA,QAAA,EAAA,EAAA,CAAA,CADA;AAEA,QAAA,MAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA,EAHA;AAIA,QAAA,YAAA,EAAA,EAJA;AAKA,QAAA,QAAA,EAAA;AALA,OAAA;AAOA,WAAA,OAAA,GAAA,IAAA;AACA,KA1BA;AA2BA,IAAA,IA3BA,gBA2BA,EA3BA,EA2BA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAHA;AAIA,KAhCA;AAiCA,IAAA,IAjCA,kBAiCA;AAAA;;AACA,UAAA,KAAA,UAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA;AACA,KA3CA;AA4CA,IAAA,UA5CA,wBA4CA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,IAAA;AACA,WAAA,OAAA,CAAA,KAAA,SAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KAhDA;AAiDA,IAAA,UAjDA,sBAiDA,EAjDA,EAiDA;AAAA;;AACA,MAAA,GAAA,CAAA,UAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,SAAA;AACA,OAHA;AAIA,KAtDA;AAuDA,IAAA,cAvDA,4BAuDA;AAAA;;AACA,MAAA,aAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA;AACA,OAFA;AAGA;AA3DA,GAjBA;AA8EA,EAAA,OA9EA,qBA8EA;AACA,SAAA,KAAA;AACA,SAAA,cAAA;AACA;AAjFA,CAAA","sourcesContent":["<template>\n  <div class=\"teacher-wrap\">\n    <div class=\"crumbs\">\n      <el-breadcrumb separator=\"/\">\n        <el-breadcrumb-item>\n          <i class=\"el-icon-fa fa-user-plus\"></i> 教师管理\n        </el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n\n    <div class=\"container\">\n      <div class=\"query-form\">\n        <el-row :gutter=\"20\">\n          <el-col :span=\"2\">\n            <el-button @click=\"create\" icon=\"el-icon-plus\">创建</el-button>\n          </el-col>\n          <el-col :offset=\"13\" :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"教师\"\n              v-model=\"queryForm.name\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"系名\"\n              v-model=\"queryForm.departmentName\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-button @click=\"query\" icon=\"el-icon-search\" type=\"primary\"\n              >搜索\n            </el-button>\n          </el-col>\n        </el-row>\n      </div>\n\n      <div class=\"table\">\n        <el-table :data=\"tableData\" stripe>\n          <el-table-column label=\"教师Id\" prop=\"id\" />\n          <el-table-column label=\"教师工号\" prop=\"number\" />\n          <el-table-column label=\"教师姓名\" prop=\"name\" />\n          <el-table-column label=\"所属系\" prop=\"departmentName\" />\n          <el-table-column align=\"center\" label=\"操作\" width=\"200px\">\n            <template slot-scope=\"scope\">\n              <el-button @click=\"edit(scope.row.id)\" size=\"mini\" type=\"success\"\n                >编辑\n              </el-button>\n              <el-button\n                @click=\"deleteItem(scope.row.id)\"\n                size=\"mini\"\n                type=\"danger\"\n                >删除\n              </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <br />\n      <el-row justify=\"center\" type=\"flex\">\n        <el-pagination\n          :current-page.sync=\"pageIndex\"\n          :page-size=\"pageSize\"\n          :total=\"pageSize * pageCount\"\n          @current-change=\"getPage\"\n          background\n          layout=\"prev, pager, next\"\n        >\n        </el-pagination>\n      </el-row>\n      <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n        <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n          <el-form-item label=\"教师工号\">\n            <el-input type=\"number\" v-model=\"entityForm.number\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"教师姓名\">\n            <el-input v-model=\"entityForm.name\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"所属系\">\n            <el-select placeholder=\"请选择系\" v-model=\"entityForm.departmentId\">\n              <el-option\n                :key=\"index\"\n                :label=\"item.name\"\n                :value=\"item.id\"\n                v-for=\"(item, index) in departments\"\n              />\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"登录密码\">\n            <el-input type=\"password\" v-model=\"entityForm.password\"></el-input>\n          </el-form-item>\n        </el-form>\n        <span class=\"dialog-footer\" slot=\"footer\">\n          <el-button @click=\"save\" type=\"primary\">确 定</el-button>\n          <el-button @click=\"editing = false\">取 消</el-button>\n        </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/teacher\";\nimport * as departmentApi from \"../../api/admin/department\";\n\nexport default {\n  name: \"AdminTeacher\",\n  data() {\n    return {\n      queryForm: {\n        departmentName: \"\",\n        name: \"\",\n      },\n      entityForm: {},\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n      editing: false,\n      departments: [],\n    };\n  },\n  methods: {\n    query() {\n      api\n        .getPageCount(this.queryForm.departmentName, this.queryForm.name)\n        .then((res) => {\n          this.pageCount = res;\n          this.pageIndex = 1;\n          this.getPage(1);\n        });\n    },\n    getPage(pageIndex) {\n      api\n        .getPage(pageIndex, this.queryForm.departmentName, this.queryForm.name)\n        .then((res) => {\n          this.tableData = res;\n        });\n    },\n    create() {\n      this.entityForm = {\n        id: -1,\n        number: \"\",\n        name: \"\",\n        departmentId: \"\",\n        password: \"\",\n      };\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then((res) => {\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    },\n    getDepartments() {\n      departmentApi.listName().then((res) => {\n        this.departments = res;\n      });\n    },\n  },\n  created() {\n    this.query();\n    this.getDepartments();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/admin"}]}