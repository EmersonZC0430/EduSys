ACA,MAAA,GAAA,CACA,OADA,CAEA,SAFA,EAGA,KAAA,SAAA,CAAA,SAHA,EAIA,KAAA,SAAA,CAAA,SAJA,EAKA,KAAA,SAAA,CAAA,IALA,EAOA,IAPA,CAOA,UAAA,GAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,GAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,UAAA,GAAA,CAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,CAAA,CAAA,CAAA,CAAA,GAAA,KAAA,CAAA,GAAA,GAAA,GAAA,GAAA;AACA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA;AACA,OAZA;AAaA,KA5BA;AA6BA,IAAA,MA7BA,oBA6BA;AACA,WAAA,UAAA,GAAA;AACA,QAAA,EAAA,EAAA,CAAA,CADA;AAEA,QAAA,MAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA,EAHA;AAIA,QAAA,OAAA,EAAA,EAJA;AAKA,QAAA,QAAA,EAAA,EALA;AAMA,QAAA,KAAA,EAAA,IANA;AAOA,QAAA,QAAA,EAAA,IAPA;AAQA,QAAA,GAAA,EAAA;AARA,OAAA;AAUA,WAAA,OAAA,GAAA,IAAA;AACA,KAzCA;AA0CA,IAAA,IA1CA,gBA0CA,EA1CA,EA0CA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAHA;AAIA,KA/CA;AAgDA,IAAA,IAhDA,kBAgDA;AAAA;;AACA,UAAA,KAAA,UAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAFA;AAGA;AACA,KA1DA;AA2DA,IAAA,UA3DA,wBA2DA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,IAAA;AACA,WAAA,OAAA,CAAA,KAAA,SAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KA/DA;AAgEA,IAAA,UAhEA,sBAgEA,EAhEA,EAgEA;AAAA;;AACA,MAAA,GAAA,CAAA,UAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,SAAA;AACA,OAHA;AAIA,KArEA;AAsEA,IAAA,UAtEA,wBAsEA;AAAA;;AACA,MAAA,QAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA;AACA,OAFA;AAGA;AA1EA,GAlBA;AA8FA,EAAA,OA9FA,qBA8FA;AACA,SAAA,KAAA;AACA,SAAA,UAAA;AACA;AAjGA,CAAA","sourcesContent":["<template>\n  <div class=\"student-wrap\">\n    <div class=\"crumbs\">\n      <el-breadcrumb separator=\"/\">\n        <el-breadcrumb-item>\n          <i class=\"el-icon-fa fa-user\"></i> 学生管理\n        </el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n\n    <div class=\"container\">\n      <div class=\"query-form\">\n        <el-row :gutter=\"20\">\n          <el-col :span=\"2\">\n            <el-button @click=\"create\" icon=\"el-icon-plus\">创建</el-button>\n          </el-col>\n          <el-col :offset=\"10\" :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"学生姓名\"\n              v-model=\"queryForm.name\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"专业名\"\n              v-model=\"queryForm.majorName\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-input\n              @keyup.enter.native=\"query\"\n              placeholder=\"班级名\"\n              v-model=\"queryForm.className\"\n            />\n          </el-col>\n          <el-col :span=\"3\">\n            <el-button @click=\"query\" icon=\"el-icon-search\" type=\"primary\"\n              >搜索\n            </el-button>\n          </el-col>\n        </el-row>\n      </div>\n\n      <div class=\"table\">\n        <el-table :data=\"tableData\" stripe max-height=\"500\">\n          <el-table-column label=\"学生Id\" prop=\"id\" width=\"80px\" />\n          <el-table-column label=\"学号\" prop=\"number\" />\n          <el-table-column label=\"姓名\" prop=\"name\" />\n          <el-table-column label=\"班级\" prop=\"className\" />\n          <el-table-column label=\"专业\" min-width=\"150px\" prop=\"majorName\" />\n          <el-table-column label=\"性别\" prop=\"sex\" width=\"80px\" />\n          <el-table-column\n            label=\"上次登录\"\n            prop=\"lastLoginTime\"\n            width=\"130px\"\n          />\n          <el-table-column align=\"center\" label=\"操作\" width=\"200px\">\n            <template slot-scope=\"scope\">\n              <el-button @click=\"edit(scope.row.id)\" size=\"mini\" type=\"success\"\n                >编辑\n              </el-button>\n              <el-button\n                @click=\"deleteItem(scope.row.id)\"\n                size=\"mini\"\n                type=\"danger\"\n                >删除\n              </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <br />\n      <el-row justify=\"center\" type=\"flex\">\n        <el-pagination\n          :current-page.sync=\"pageIndex\"\n          :page-size=\"pageSize\"\n          :total=\"pageSize * pageCount\"\n          @current-change=\"getPage\"\n          background\n          layout=\"prev, pager, next\"\n        >\n        </el-pagination>\n      </el-row>\n      <el-dialog :visible.sync=\"editing\" title=\"编辑\" width=\"30%\">\n        <el-form :model=\"entityForm\" label-width=\"70px\" ref=\"form\">\n          <el-form-item label=\"姓名\">\n            <el-input v-model=\"entityForm.name\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"学号\">\n            <el-input type=\"number\" v-model=\"entityForm.number\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"所属班级\">\n            <el-select placeholder=\"请选择班级\" v-model=\"entityForm.classId\">\n              <el-option\n                :key=\"index\"\n                :label=\"item.name\"\n                :value=\"item.id\"\n                v-for=\"(item, index) in classes\"\n              />\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"密码\">\n            <el-input type=\"password\" v-model=\"entityForm.password\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"邮箱\">\n            <el-input v-model=\"entityForm.email\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"生日\">\n            <el-date-picker\n              format=\"yyyy-MM-dd\"\n              placeholder=\"选择生日\"\n              type=\"date\"\n              v-model=\"entityForm.birthday\"\n            >\n            </el-date-picker>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio-group v-model=\"entityForm.sex\">\n              <el-radio :label=\"1\">男</el-radio>\n              <el-radio :label=\"0\">女</el-radio>\n            </el-radio-group>\n          </el-form-item>\n        </el-form>\n        <span class=\"dialog-footer\" slot=\"footer\">\n          <el-button @click=\"save\" type=\"primary\">确 定</el-button>\n          <el-button @click=\"editing = false\">取 消</el-button>\n        </span>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"../../api/admin/student\";\nimport * as classApi from \"../../api/admin/class\";\n\nexport default {\n  name: \"AdminStudent\",\n  data() {\n    return {\n      queryForm: {\n        majorName: \"\",\n        className: \"\",\n        name: \"\",\n      },\n      entityForm: {},\n      tableData: [],\n      pageSize: api.pageSize,\n      pageCount: 1,\n      pageIndex: 1,\n      editing: false,\n      classes: [],\n    };\n  },\n  methods: {\n    query() {\n      api\n        .getPageCount(\n          this.queryForm.majorName,\n          this.queryForm.className,\n          this.queryForm.name\n        )\n        .then((res) => {\n          this.pageCount = res;\n          this.pageIndex = 1;\n          this.getPage(1);\n        });\n    },\n    getPage(pageIndex) {\n      api\n        .getPage(\n          pageIndex,\n          this.queryForm.majorName,\n          this.queryForm.className,\n          this.queryForm.name\n        )\n        .then((res) => {\n          for (let i = 0; i < res.length; i++) {\n            res[i].sex = res[i].sex === 1 ? \"男\" : \"女\";\n          }\n          this.tableData = res;\n        });\n    },\n    create() {\n      this.entityForm = {\n        id: -1,\n        number: \"\",\n        name: \"\",\n        classId: \"\",\n        password: \"\",\n        email: null,\n        birthday: null,\n        sex: 0,\n      };\n      this.editing = true;\n    },\n    edit(id) {\n      api.get(id).then((res) => {\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      if (this.entityForm.id === -1) {\n        api.create(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      } else {\n        api.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    },\n    getClasses() {\n      classApi.listName().then((res) => {\n        this.classes = res;\n      });\n    },\n  },\n  created() {\n    this.query();\n    this.getClasses();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/admin"}]}